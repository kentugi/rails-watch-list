<div class="container">

<div class="card-category" style="background-image: linear-gradient(rgba(0,0,0,0.3), rgba(0,0,0,0.3)), url(<%= cl_image_path @list.photo.key, height: 300, width: 400, crop: :fill %>)">
  <%= @list.name %> %>
</div>

<ul>
<li><% @list.bookmarks.each do |bookmark| %>
<%= bookmark.movie.title %> - <%= bookmark.comment %>
<% end %>
</li>
</ul>
<p><%= link_to 'Go back', lists_path %></p>
<p><%= link_to 'Add bookmark', new_list_bookmark_path(@list.id) %></p>

</div>

